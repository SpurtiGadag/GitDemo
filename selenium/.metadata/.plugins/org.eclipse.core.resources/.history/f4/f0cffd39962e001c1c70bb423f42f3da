package javaPrac;

public class Pallindromepgm {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="madam";
		String t = null;
		for (int i=s.length()-1;i>=0;i--)
		{
			t= t+s.charAt(i);
			
		}
		if(t==s)
			System.out.println("Pallindrome");
		else
		{
			System.out.println("Not Pallindrome");
		}
	}

}
